{
  "name": "celysium/cache",
  "description": "Cache manager ",
  "license": "MIT",
  "keywords": [
    "cache",
    "redis"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Nasser Hekmati",
      "email": "hekmati.nasser@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": ">11.0"
  },
  "autoload": {
    "psr-4": {
      "Celysium\\Cache\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Celysium\\Cache\\CacheServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
